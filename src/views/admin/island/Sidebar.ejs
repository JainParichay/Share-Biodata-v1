        </a>
      </div>

      <nav class="flex-1 space-y-1 bg-white px-2 py-4">
        <!-- Dashboard -->
        <a
          href="/<%= adminKey ? '?adminKey=' + adminKey : '' %>"
          class="group flex items-center px-2 py-2 text-sm font-medium rounded-md <%= path === '/admin' ? 'bg-gray-100 text-gray-900' : 'text-gray-600 hover:bg-gray-50 hover:text-gray-900' %>"
