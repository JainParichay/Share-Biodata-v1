        <a
          href="/share<%= adminKey ? '?adminKey=' + adminKey : '' %>"
          class="group flex items-center px-2 py-2 text-sm font-medium rounded-md <%= path === '/admin/share' ? 'bg-gray-100 text-gray-900' : 'text-gray-600 hover:bg-gray-50 hover:text-gray-900' %>"
        >
          <svg
            class="mr-3 h-6 w-6"
